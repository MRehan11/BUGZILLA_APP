  
  
  <%= bootstrap_form_with(model: @project,  local: true, label_errors: true, inline_errors: true) do |f| %>
    <%= f.text_field :title, style: 'width:50%;' %>
    <%= f.text_field :description, style: 'width:50%;' %>
    <%= f.collection_select(:user_ids, User.developer, :id, :name, {prompt: "Select Developers" }, {:multiple => true, class: "form-control", style: "width:20%"} ) %>
    <%= f.submit %>
  <% end %>